            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".BeefBrisketActivity"/>
        <activity android:name=".ChickenTeriyakiActivity"/>
        <activity android:name=".LomiActivity"/>
        <activity android:name=".NachosActivity"/>
        <activity android:name=".PotatoCasseroleActivity"/>
        <activity android:name=".SisigActivity"/>
        <activity android:name=".SpaghettiActivity"/>
        <activity android:name=".SpamMusubiActivity"/>
        <activity android:name=".AboutActivity"/>
        <activity android:name=".GalleryActivity"/>

    </application>

</manifest>
